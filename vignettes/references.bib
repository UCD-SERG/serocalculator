@article{Noordzij2010diseasemeasures,
    author = {Noordzij, Marlies and Dekker, Friedo W. and Zoccali, Carmine and Jager, Kitty J.},
    title = "{Measures of Disease Frequency: Prevalence and Incidence}",
    journal = {Nephron Clinical Practice},
    volume = {115},
    number = {1},
    pages = {c17-c20},
    year = {2010},
    month = {02},
    abstract = "{To describe how often a disease or another health event occurs in a population, different measures of disease frequency can be used. The prevalence reflects the number of existing cases of a disease. In contrast to the prevalence, the incidence reflects the number of new cases of disease and can be reported as a risk or as an incidence rate. Prevalence and incidence are used for different purposes and to answer different research questions. In this article, we discuss the different measures of disease frequency and we explain when to apply which measure.}",
    issn = {1660-2110},
    doi = {10.1159/000286345},
